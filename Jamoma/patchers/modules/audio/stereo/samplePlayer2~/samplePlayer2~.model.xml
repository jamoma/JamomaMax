<?xml version='1.0' encoding='iso-8859-1' standalone='yes' ?>

<jamoma version='0.6'
		xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
		xsi:schemaLocation='http://jamoma.org/ file:jamoma.xsd'
>
  <preset name='default'>
    <command address="audio/gain">100.000000</command>
    <command address="audio/mute">0</command>
    <command address="loop">0</command>
    <command address="loopsync/active">0</command>
    <command address="loopsync/mode">loop_time</command>
    <command address="reverse">0</command>
    <command address="sample/start">0</command>
    <command address="sample/stop">0</command>
    <command address="sample/vzoom">1.</command>
    <command address="speed/active">0</command>
    <command address="speed/ratio">1.000000</command>  
    <command address="pitch/ratio">0.0000</command>  
    <command address="pitch/active">0</command>
    <command address="view/freeze">0</command>
  </preset>
</jamoma>
