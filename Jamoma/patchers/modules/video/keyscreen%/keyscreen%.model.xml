<?xml version='1.0' encoding='iso-8859-1' standalone='yes' ?>

<jamoma version='0.6'
		xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
		xsi:schemaLocation='http://jamoma.org/ file:jamoma.xsd'
>
  <preset name='default'>
    <command address="video/preview">0</command>
    <command address="video/mute">0</command>
    <command address="video/freeze">0</command>
    <command address="video/bypass">0</command>
    <command address="tolerance">0.5</command>
    <command address="target">2</command>
    <command address="mode">0</command>
    <command address="mask">3</command>
    <command address="key">1</command>
    <command address="key/red">0.5</command>
    <command address="key/green">0.5</command>
    <command address="key/blue">0.5</command>
  </preset>
</jamoma>
