<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<jamoma version="0.6" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jamoma.org/ file:jamoma.xsd">
 <cue name="Init">
  <flag name="description">"Initialisation. Models are ready to play."</flag>
  <!---->
  <script address="mouse">
   <command address="reference">screen</command>
   <command address="active">0</command>
  </script>
  <!---->
  <script address="degrade~">
   <command address="samplerateRatio">1.000000</command>
   <command address="bitdepth">24</command>
   <script address="audio">
    <command address="gain">100.000000</command>
    <command address="mix">0.000000</command>
    <command address="bypass">0</command>
    <command address="mute">0</command>
   </script>
  </script>
 </cue>
 <cue name="Act I - Scene I">
  <flag name="description">"Mouse mapping activated + audio FX applied"</flag>
  <!---->
  <script address="mouse">
   <command address="reference">screen</command>
   <command address="active">1</command>
  </script>
  <!---->
  <script address="degrade~">
   <command address="samplerateRatio">1.000000</command>
   <command address="bitdepth">24</command>
   <script address="audio">
    <command address="gain">100.000000</command>
    <command address="mix">100.000000</command>
    <command address="bypass">0</command>
    <command address="mute">0</command>
   </script>
  </script>
 </cue>
 <cue name="Act I - Scene II">
  <flag name="description">"audio degrade get harsher"</flag>
  <!---->
  <script address="mouse">
   <command address="reference">screen</command>
   <command address="active">1</command>
  </script>
  <!---->
  <script address="degrade~">
   <command address="samplerateRatio">0.100000</command>
   <command address="bitdepth">1</command>
   <script address="audio">
    <command address="gain">80.000000</command>
    <command address="mix">100.000000</command>
    <command address="bypass">0</command>
    <command address="mute">0</command>
   </script>
  </script>
 </cue>
 <cue name="Outro">
  <flag name="description">"Slow fadeout toward the end of the show"</flag>
  <!---->
  <script address="mouse">
   <command address="reference">screen</command>
   <command address="active">0</command>
  </script>
  <!---->
  <script address="degrade~">
   <command address="samplerateRatio">0.100000</command>
   <command address="bitdepth">1</command>
   <script address="audio">
    <command address="gain">80.000000</command>
    <command address="mix">0.000000</command>
    <command address="bypass">0</command>
    <command address="mute">0</command>
   </script>
  </script>
 </cue>
</jamoma>
